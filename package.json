{
  "name": "dans-ui",
  "version": "0.1.5",
  "description": "React components library",
  "private": false,
  "main": "dist/index.es.js",
  "module": "dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "files": [
    "dist"
  ],
  "sideEffects": [
    "dist/index.css"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react-transition-group": "^4.4.10",
    "classnames": "^2.5.1",
    "react-transition-group": "^4.4.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "build": "npm run clean && npm run build-es",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-ts": "tsc -p tsconfig.build.json",
    "build-css": "sass ./src/styles/index.scss ./dist/index.css",
    "prepublishOnly": "npm run build",
    "build-es": "rollup --config rollup/rollup.esm.config.js",
    "build-umd": "rollup --config rollup/rollup.umd.config.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@storybook/addon-docs": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.11",
    "@storybook/addon-interactions": "^7.6.11",
    "@storybook/addon-links": "^7.6.11",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/blocks": "^7.6.11",
    "@storybook/preset-create-react-app": "^7.6.11",
    "@storybook/react": "^7.6.11",
    "@storybook/react-webpack5": "^7.6.11",
    "@storybook/test": "^7.6.11",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/classnames": "^2.3.1",
    "async-validator": "^4.2.5",
    "eslint-plugin-storybook": "^0.6.15",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rimraf": "^5.0.5",
    "rollup": "^2.79.1",
    "rollup-plugin-exclude-dependencies-from-bundle": "^1.1.23",
    "rollup-plugin-sass": "^1.12.21",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.70.0",
    "storybook": "^7.6.11",
    "web-vitals": "^2.1.4",
    "webpack": "^5.90.0"
  }
}
